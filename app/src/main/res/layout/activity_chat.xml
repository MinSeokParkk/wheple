<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <TextView
            android:text="채팅 문의"
            android:textColor="@color/black"
            android:textSize="18dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView118" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/guideline10"/>
    <ImageView
            android:layout_width="22dp"
            android:layout_height="22dp" app:srcCompat="@drawable/icon_back"
            android:id="@+id/imageView5"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline10"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline10" app:layout_constraintGuide_begin="46dp"
                                          android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            app:layout_constraintTop_toTopOf="@+id/guideline10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
    >

        <ImageView
                android:scaleType="centerCrop"
                android:layout_width="70dp"
                android:layout_height="50dp" app:srcCompat="@drawable/img_gym"
                android:id="@+id/imageView18" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="28dp" android:layout_marginStart="28dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
        <TextView
                android:text="건국대학교 실내체육관"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView119" android:textSize="15dp"
                app:layout_constraintStart_toEndOf="@+id/imageView18" android:layout_marginLeft="24dp"
                android:layout_marginStart="24dp" android:layout_marginTop="18dp"
                app:layout_constraintTop_toTopOf="parent"/>
        <TextView
                android:text="\uf041"
                android:textSize="13dp"
                android:fontFamily="@font/fontawesome_webfont"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView120" app:layout_constraintStart_toEndOf="@+id/imageView18"
                android:layout_marginLeft="24dp" android:layout_marginStart="24dp" android:layout_marginTop="9dp"
                app:layout_constraintTop_toBottomOf="@+id/textView119"/>
        <TextView
                android:textColor="@color/white"
                android:text="서울 광진구"
                android:textSize="12dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView121" app:layout_constraintStart_toEndOf="@+id/textView120"
                android:layout_marginLeft="9dp" android:layout_marginStart="9dp" android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/textView119"/>
        <TextView
                android:text="\uf105"
                android:textSize="30dp"
                android:fontFamily="@font/fontawesome_webfont"
                android:textColor="@color/white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView122" app:layout_constraintStart_toEndOf="@+id/textView119"
                android:layout_marginStart="20dp" android:layout_marginLeft="20dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="183dp"
                android:layout_height="61dp"
                app:layout_constraintStart_toEndOf="@+id/imageView18" android:layout_marginLeft="19dp"
                android:layout_marginStart="19dp" android:layout_marginTop="8dp"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent" android:id="@+id/backtoSpace_layout">
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <EditText
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:paddingLeft="50dp"
            android:paddingRight="50dp"
            android:paddingVertical="8dp"
            android:textSize="12dp"
            android:background="@drawable/edittext_background"
            android:inputType="textMultiLine"

            android:ems="10"
            android:id="@+id/editText13" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:hint="메세지 입력" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageView
            android:layout_width="33dp"
            android:layout_height="33dp" app:srcCompat="@drawable/icon_send"
            android:id="@+id/imageView19"
            app:layout_constraintTop_toTopOf="@+id/editText13"
            app:layout_constraintBottom_toBottomOf="@+id/editText13" app:layout_constraintEnd_toEndOf="@+id/editText13"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp" app:srcCompat="@drawable/icon_camera"
            android:id="@+id/imageView20"
            app:layout_constraintTop_toTopOf="@+id/editText13"
            app:layout_constraintBottom_toBottomOf="@+id/editText13"
            app:layout_constraintStart_toStartOf="@+id/editText13" android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp" android:layout_marginTop="1dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>