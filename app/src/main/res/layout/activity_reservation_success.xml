<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="\uf274"
            android:textSize="75dp"
            android:textColor="@color/colorPrimary"
            android:fontFamily="@font/fontawesome_webfont"
            android:id="@+id/textView78" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="140dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.52"/>
    <TextView
            android:text="예약이 완료되었습니다!"
            android:textSize="23dp"
            android:textColor="@color/colorPrimary"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView95" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="31dp" app:layout_constraintTop_toBottomOf="@+id/textView78"/>
    <TextView
            android:text="건국대학교 실내체육관"
            android:textColor="@color/black"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView97" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView95" android:textSize="18dp"/>
    <Button
            android:text="홈으로 가기"
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:id="@+id/home_Button" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="29dp"
            android:background="@drawable/button_on"
            android:textColor="@color/white"
            app:layout_constraintTop_toBottomOf="@+id/button_goMyres"/>
    <Button
            android:text="예약내역 보기"
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:id="@+id/button_goMyres"
            android:background="@drawable/button_on"
            android:textColor="@color/white"
            android:layout_marginTop="75dp"
            app:layout_constraintTop_toBottomOf="@+id/textView97" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
    />
</androidx.constraintlayout.widget.ConstraintLayout>