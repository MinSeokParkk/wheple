<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
             android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_chatlist_guest"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:text="로그인이 필요한 페이지입니다."
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView123" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="254dp"
               />
        <Button
                android:text="로그인/회원가입"
                android:background="@drawable/button_on"
                android:textColor="@color/white"
                android:layout_width="192dp"
                android:layout_height="49dp"
                android:id="@+id/chatlist_login_button" android:layout_marginTop="22dp"
                app:layout_constraintTop_toBottomOf="@+id/textView123" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
        />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraint_chatlist_user"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                              android:id="@+id/guideline12" app:layout_constraintGuide_begin="46dp"
                                              android:orientation="horizontal"/>
        <TextView
                android:text="채팅 문의"
                android:textColor="@color/black"
                android:textSize="18dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView107" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/guideline12"/>

        <View
                android:layout_width="match_parent"
                android:layout_height="0.3dp"
                android:background="#A8A8A8"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/view8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintBottom_toTopOf="@+id/constraintLayout10"/>

        <androidx.constraintlayout.widget.ConstraintLayout

                android:layout_width="match_parent"
                android:layout_height="130dp"
                app:layout_constraintTop_toTopOf="@+id/guideline12" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:id="@+id/constraintLayout10">

            <ImageView
                    android:scaleType="centerCrop"
                    android:layout_width="70dp"
                    android:layout_height="70dp" app:srcCompat="@drawable/img_gym"
                    android:id="@+id/imageView3"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginStart="32dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="32dp"
                    app:layout_constraintBottom_toBottomOf="parent"/>
            <TextView
                    android:text="건국대학교 실내체육관"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView108" app:layout_constraintStart_toEndOf="@+id/imageView3"
                    android:layout_marginLeft="24dp" android:layout_marginStart="24dp" android:layout_marginTop="27dp"
                    app:layout_constraintTop_toTopOf="parent"/>
            <TextView

                    android:text="\uf041"
                    android:textSize="15dp"

                    android:fontFamily="@font/fontawesome_webfont"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView109" app:layout_constraintStart_toEndOf="@+id/imageView3"
                    android:layout_marginLeft="24dp" android:layout_marginStart="24dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView108" android:layout_marginTop="3dp"/>
            <TextView
                    android:text="서울 광진구"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView110" android:layout_marginLeft="6dp" android:layout_marginStart="6dp"
                    app:layout_constraintStart_toEndOf="@+id/textView109" android:layout_marginTop="2dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView108" android:textSize="12dp"/>
            <TextView
                    android:text="안녕하세요! 문의 답변 드립니다. 저희 건국대학교 실내체육관은 농구, 배드민턴 등 다양한 실내 스포츠를"
                    android:textSize="12dp"
                    android:textColor="@color/black"
                    android:layout_width="254dp"
                    android:layout_height="30dp"
                    android:id="@+id/textView111" app:layout_constraintStart_toEndOf="@+id/imageView3"
                    android:layout_marginLeft="24dp" android:layout_marginStart="24dp" android:layout_marginTop="6dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView110"/>
            <TextView
                    android:text="오전 11:20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView112" android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="27dp" android:textSize="10dp"/>
            <View
                    android:layout_width="match_parent"
                    android:layout_height="0.3dp"
                    android:background="#A8A8A8"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:id="@+id/view9"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintBottom_toBottomOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="130dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:id="@+id/constraintLayout11"
                app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toBottomOf="@+id/constraintLayout10"
        >

            <ImageView
                    android:scaleType="centerCrop"
                    android:layout_width="70dp"
                    android:layout_height="70dp" app:srcCompat="@drawable/img_futsal"
                    android:id="@+id/img_home_reset"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                    android:text="용산 아이파크 풋살장"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView113"
                    app:layout_constraintStart_toEndOf="@+id/img_home_reset"
                    android:layout_marginStart="24dp" android:layout_marginLeft="24dp" android:layout_marginTop="27dp"
                    app:layout_constraintTop_toTopOf="parent"/>
            <TextView

                    android:text="\uf041"
                    android:textSize="15dp"

                    android:fontFamily="@font/fontawesome_webfont"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView114"
                    app:layout_constraintStart_toEndOf="@+id/img_home_reset"
                    android:layout_marginLeft="24dp" android:layout_marginStart="24dp" android:layout_marginTop="3dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView113"/>
            <TextView
                    android:text="서울 용산구"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView115"
                    android:textSize="12dp"
                    app:layout_constraintStart_toEndOf="@+id/textView114"
                    android:layout_marginLeft="6dp" android:layout_marginStart="6dp" android:layout_marginTop="2dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView113"/>
            <TextView
                    android:text="야간 조명 가능한가요?"
                    android:textSize="12dp"
                    android:textColor="@color/black"
                    android:layout_width="254dp"
                    android:layout_height="30dp"
                    android:id="@+id/textView116"
                    app:layout_constraintStart_toEndOf="@+id/img_home_reset"
                    android:layout_marginLeft="24dp" android:layout_marginStart="24dp" android:layout_marginTop="6dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView114"/>
            <TextView
                    android:text="2019-08-20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView117"
                    android:textSize="10dp" app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="27dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="32dp" android:layout_marginRight="32dp"/>
            <View
                    android:layout_width="match_parent"
                    android:layout_height="0.3dp"
                    android:background="#A8A8A8"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:id="@+id/view10"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>