<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent"
             android:layout_height="match_parent">
    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraint_mypage_guest"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="로그인이 필요한 페이지입니다."
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView123" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="254dp"
                app:layout_constraintHorizontal_bias="0.497"/>
        <Button
                android:text="로그인/회원가입"
                android:layout_width="178dp"
                android:layout_height="49dp"
                android:id="@+id/mypage_login_button" android:layout_marginTop="22dp"
                app:layout_constraintTop_toBottomOf="@+id/textView123" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
        />
    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraint_mypage_user"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <Button
                android:text="로그아웃"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/logout_Button"
                android:layout_marginTop="28dp"
                app:layout_constraintTop_toBottomOf="@+id/textView126" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="152dp" android:layout_marginStart="152dp"/>
        <TextView
                android:text="마이페이지 (임시)"
                android:textSize="24dp"
                android:textColor="@color/colorPrimary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView126"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="108dp"
                android:layout_marginStart="108dp" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="84dp"/>
    </android.support.constraint.ConstraintLayout>
</FrameLayout>